# .env example file

# RabbitMQ Configuration
RABBITMQ_HOST=localhost               # RabbitMQ host (e.g., localhost, or URL to your RabbitMQ service)
RABBITMQ_PORT=5672                    # RabbitMQ default port (if using default port)
RABBITMQ_USER=guest                   # RabbitMQ username (default is "guest")
RABBITMQ_PASSWORD=guest               # RabbitMQ password (default is "guest")
RABBITMQ_QUEUE=myQueue                # The name of the queue to listen to

# Database Configuration
DB_HOST=localhost                     # Database host (e.g., localhost, or the database server URL)
DB_PORT=5432                          # Database port (default is usually 5432 for PostgreSQL)
DB_USER=myDatabaseUser                # Database username
DB_PASS=myDatabasePassword            # Database password
DB_NAME=myDatabaseName                # The name of the database to connect to

# Application Configuration
APP_ENV=development                   # Application environment (e.g., development, production)
APP_PORT=8080                         # Port on which the service will run (if applicable)
LOG_LEVEL=info                        # Log level (e.g., info, debug, warn, error)

# Other Configuration
API_KEY=yourApiKeyHere                # API key for other services, if needed
CACHE_HOST=localhost                  # Host for caching service, if used
CACHE_PORT=6379                       # Port for caching service (e.g., Redis default port)
